<script>
    $(document).ready(function (e) {
        $.get("/Post/_IndexB" , function (respuesta) {
            $("#tbody").html(respuesta);
        });
        e.preventDefault();
    });
</script>



<div id="tbody">

</div>

<script>
    $("#busqueda").keyup(function (e) {
        $.get("/Post/_IndexB?busqueda=" + $("#busqueda").val(), function (respuesta) {
            $("#tbody").html(respuesta);
        });
        e.preventDefault();
    });
</script>

